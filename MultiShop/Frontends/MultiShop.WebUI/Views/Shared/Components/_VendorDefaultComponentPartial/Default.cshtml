@model List<ResultBrandDto>


<div class="container-fluid py-5">
    <div class="row px-xl-5">
        <div class="col">
            <div class="owl-carousel vendor-carousel">
                @foreach (var item in Model)
                {
                   
                        <div class="d-flex flex-nowrap bg-light p-2" style="height:100px; width:150px;">
                            <img  src="@item.BrandImageUrl" alt="@item.BrandName ">
                        </div>
                }
            </div>
        </div>
    </div>
</div>